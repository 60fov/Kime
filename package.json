{
  "name": "kime",
  "version": "0.0.1",
  "description": "Korean virtual IME",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/kime.umd.cjs",
  "module": "./dist/kime.js",
  "types": "./dist/kime.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kime.js",
      "require": "./dist/kime.umd.cjs"
    },
    "./hooks": {
      "import": "./dist/hooks/useKime.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/60fov/Kime.git"
  },
  "author": "60fov",
  "bugs": {
    "url": "https://github.com/60fov/Kime/issues"
  },
  "homepage": "https://github.com/60fov/Kime#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-dts": "^1.0.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.2"
  }
}